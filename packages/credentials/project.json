{
  "name": "credentials",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/credentials/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/credentials",
        "main": "packages/credentials/src/index.ts",
        "tsConfig": "packages/credentials/tsconfig.lib.json",
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": ["packages/credentials/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/credentials/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/credentials/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
